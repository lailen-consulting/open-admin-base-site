{
    "name": "lailen/site",
    "description": "Simple basic Open admin extension for building websites",
    "type": "library",
    "keywords": ["open-admin", "extension"],
    "homepage": "https://github.com/lailen/site",
    "license": "MIT",
    "authors": [
        {
            "name": "Francis Lalnunmawia",
            "email": "francis@lailen.com"
        }
    ],
    "require": {
        "php": ">=7.0.0",
        "intervention/image": "^2.7",
        "open-admin-ext/ckeditor": "^1.0",
        "open-admin-ext/media-manager": "^1.0",
        "open-admin-org/open-admin": "~1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~6.0"
    },
    "autoload": {
        "psr-4": {
            "Lailen\\OpenAdmin\\Site\\": "src/",
            "Lailen\\Seeders\\": "database/seeders/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Lailen\\OpenAdmin\\Site\\SiteServiceProvider"
            ]
        }
    }
}
